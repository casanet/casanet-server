{
  "name": "casa-net-ui",
  "version": "0.0.0",
  "scripts": {
    "environment:internal": "node ./src/environments/build-internal-environment.js",
    "environment:external": "node ./src/environments/build-external-environment.js",
    "postngbuild": "copyfiles ./light-app/**/* ./dist && node ./pwa/pwa-ngsw-copy.js",
    "build": "npm run environment:internal && ng build --prod && npm run postngbuild && del-cli ../backend/dist/public/ --force && copyfiles -u 1 ./dist/**/* ../backend/dist/public",
    "build:external": "npm run environment:external && ng build --prod && npm run postngbuild",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "prebuild": "node ./pwa/pwa-versioning.js"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "^7.2.15",
    "@angular/cdk": "^7.3.7",
    "@angular/common": "^7.2.15",
    "@angular/compiler": "^7.2.15",
    "@angular/core": "^7.2.15",
    "@angular/flex-layout": "7.0.0-beta.19",
    "@angular/forms": "^7.2.15",
    "@angular/http": "^7.2.15",
    "@angular/material": "^7.3.7",
    "@angular/platform-browser": "^7.2.15",
    "@angular/platform-browser-dynamic": "^7.2.15",
    "@angular/pwa": "^0.801.1",
    "@angular/router": "^7.2.15",
    "@angular/service-worker": "^7.2.14",
    "@ecodev/fab-speed-dial": "^3.1.0",
    "angular-mgl-timeline": "^0.3.2",
    "core-js": "^2.6.9",
    "hammerjs": "^2.0.8",
    "material-community-components": "^3.4.3",
    "ngx-perfect-scrollbar": "^7.2.1",
    "rxjs": "~6.3.3",
    "screenfull": "^3.3.3",
    "sweetalert2": "^7.33.1",
    "zone.js": "^0.8.29"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.13.9",
    "@angular/cli": "^7.3.9",
    "@angular/compiler-cli": "^7.2.15",
    "@angular/language-service": "^7.2.15",
    "@types/jasmine": "^2.8.16",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "copyfiles": "^2.1.1",
    "del-cli": "^2.0.0",
    "fs": "0.0.1-security",
    "fs-extra": "^8.1.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.2.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.5",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "^5.4.2",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.1.1"
  }
}
